{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environment": {
      "value": "staging"
    },
    "location": {
      "value": "Southeast Asia"
    },
    "namePrefix": {
      "value": "omnia"
    },
    "postgresqlAdminLogin": {
      "value": "omniaadmin"
    },
    "postgresqlAdminPassword": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/omnia-staging/providers/Microsoft.KeyVault/vaults/kv-omnia-staging"
        },
        "secretName": "postgresql-admin-password"
      }
    },
    "redisCacheSku": {
      "value": "Standard"
    },
    "eventHubSku": {
      "value": "Standard"
    }
  }
}